{
  "dataset_root": "./cifar-100-python",
  "checkpoint_dir": "./cifar_regressor/checkpoints/ablation/resnet34_cbam",
  "encoder_name": "resnet34",
  "pretrained_backbone": true,
  "use_cbam": true,
  "num_coarse": 20,
  "num_fine": 100,
  "hidden_features": 256,
  "dropout_p": 0.1,
  "use_film": true,
  "film_hidden": 256,
  "film_use_probs": true,
  "batch_size": 128,
  "epochs": 20,
  "learning_rate": 0.0003,
  "backbone_lr_mult": 0.1,
  "weight_decay": 0.05,
  "num_workers": 4,
  "log_interval": 50,
  "val_split": 0.1,
  "seed": 42,
  "device": "cuda",
  "mixed_precision": true,
  "lambda_coarse": 0.2,
  "scheduler": {
    "type": "cosine",
    "eta_min": 1e-06
  },
  "aug": {
    "use_val_preprocess": true
  }
}